{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "commerceguys/addressing": "^1.4",
        "composer/installers": "^2.2",
        "cweagans/composer-patches": "^1.7",
        "drupal/address": "^1.12",
        "drupal/admin_toolbar": "^3.4",
        "drupal/allowed_formats": "^3.0",
        "drupal/antibot": "^2.0",
        "drupal/auto_entitylabel": "^3.0",
        "drupal/autotitle": "^1.2",
        "drupal/backup_migrate": "^5.0",
        "drupal/batch_resize_image": "^2.0",
        "drupal/block_style_plugins": "^1.6",
        "drupal/block_styles": "^2.0",
        "drupal/bootstrap_barrio": "^5.5",
        "drupal/build_hooks": "^3.3",
        "drupal/cer": "^5.0@beta",
        "drupal/change_author_action": "^1.0",
        "drupal/cheeseburger_menu": "^4.4",
        "drupal/chosen": "^3.0",
        "drupal/classy": "^1.0",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_filter": "^2.6",
        "drupal/config_ignore": "^3.2",
        "drupal/config_split": "^2.0@RC",
        "drupal/config_split_ignore": "^1.9",
        "drupal/core-composer-scaffold": "^10.2.4",
        "drupal/core-project-message": "^10.2.4",
        "drupal/core-recommended": "^10.2.4",
        "drupal/custom_field": "^3.0@RC",
        "drupal/default_content": "^2.0@alpha",
        "drupal/devel": "^5.0",
        "drupal/dimension": "^2.0",
        "drupal/draggableviews": "^2.0",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/embed": "^1.6",
        "drupal/entity_browser": "^2.6",
        "drupal/entity_embed": "^1.3",
        "drupal/entity_pager": "^2.0@alpha",
        "drupal/environment_indicator_ribbon": "^1.0",
        "drupal/field_config_cardinality": "^3.0",
        "drupal/field_formatter_class": "^1.6",
        "drupal/field_formatter_range": "^1.4",
        "drupal/field_group": "^3.4",
        "drupal/file_delete": "^2.0",
        "drupal/file_entity": "^2.0@RC",
        "drupal/gatsby": "^2.0@RC",
        "drupal/geofield": "^1.38",
        "drupal/gin": "^3.0@RC",
        "drupal/gin_login": "^2.0",
        "drupal/gin_toolbar": "^1.0@RC",
        "drupal/httpswww": "^2.1",
        "drupal/iban_field": "^2.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jsonapi_extras": "^3.24",
        "drupal/jsonapi_hypermedia": "^1.9",
        "drupal/lang_dropdown": "^2.1",
        "drupal/layout_builder_extra_templates": "^1.0@RC",
        "drupal/layout_builder_styles": "^2.0",
        "drupal/limited_field_widgets": "^2.1@beta",
        "drupal/media_download": "^1.2",
        "drupal/media_entity_download": "^2.2",
        "drupal/media_pdf_thumbnail": "^6.0",
        "drupal/metatag": "^2.0",
        "drupal/migrate_sandbox": "^1.1",
        "drupal/migrate_tools": "^6.0",
        "drupal/migrate_upgrade": "^4.0",
        "drupal/modules_weight": "^3.0",
        "drupal/name": "^1.0@RC",
        "drupal/oembed_providers": "^2.1",
        "drupal/paragraphs": "^1.17",
        "drupal/pathauto": "^1.12",
        "drupal/permissions_filter": "^1.6",
        "drupal/price": "3.0.x-dev@dev",
        "drupal/quick_node_clone": "*",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/redirect": "^1.9",
        "drupal/remote_stream_wrapper": "^2.0",
        "drupal/responsive_layout_builder": "^2.1",
        "drupal/responsive_menus": "^2.0@beta",
        "drupal/shield": "^1.7",
        "drupal/stable": "^2.0",
        "drupal/stage_file_proxy": "^2.1",
        "drupal/svg_image": "^3.0",
        "drupal/swiper_formatter": "^1.0",
        "drupal/taxonomy_menu_ui": "^3.0",
        "drupal/telephone_validation": "^2.4",
        "drupal/viewsreference": "^2.0@beta",
        "drupal/webform": "^6.2",
        "drupal/webform_iban_field": "^2.0",
        "drupal/webp": "^1.0@RC",
        "drupal/yaml_editor": "^1.2",
        "drupal/yearonly": "^9.1",
        "drush/drush": "^12.5",
        "mglaman/composer-drupal-lenient": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "cweagans/composer-patches": true,
            "wikimedia/composer-merge-plugin": true,
            "php-http/discovery": true,
            "mglaman/composer-drupal-lenient": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
	    "file-mapping": {
              "[web-root]/robots.txt": false,
              "[web-root]/sites/development.services.yml": false
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "enable-patching": true,
        "patches": {
            "drupal/core": {
                "Also use text editor (CKEditor) for \"summary\" of a text field: https://www.drupal.org/project/drupal/issues/2671162": "https://www.drupal.org/files/issues/2024-03-10/2671162-143.patch"
            },
            "drupal/cer": {
                "Update existing references on save: https://www.drupal.org/project/cer/issues/3240535": "https://www.drupal.org/files/issues/2021-10-03/cer-ignore-original-entity-in-differences-calculation-2846318-14-reroll.patch"
            },
            "drupal/price": {
               "Drupal 10 compatibility fixes https://www.drupal.org/project/price/issues/3413966": "https://www.drupal.org/files/issues/2024-01-11/drupal-10-compatibility-3413966-1.patch"
            },
            "drupal/dimension": {
               "Support units in theme function": "patches/dimensions-add-unit.patch"
            }
        },
        "merge-plugin": {
          
        },
        "drupal-lenient": {
            "allowed-list": ["drupal/addressing", "drupal/price"]
        }
    }
}
